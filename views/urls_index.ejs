<!DOCTYPE html>
<html lang="en">
<header>
  <% include partials/_header.ejs %>
    <a href="/urls/new">Create a New Short Link</a>
    <% for (id in urlDatabase) {
      if (urlDatabase[id]["id"] == user_id) { %>
      <li>
        <div class="id">
          <label> <%= id %> <%= urlDatabase[id] %> : </label>
          <a href="/urls/<%= id %>"> <%= urlDatabase[id]["shortURL"] %> </a>
        <form action="/urls/<%= id %>" method="GET">
          <input type="submit" value="Edit">
        </form>
        <form action="/urls/<%= id %>/delete" method="POST">
          <input type="submit" value="Delete">
        </form>
        </div>
      </li>
    <% } } %>
</body>
</html>