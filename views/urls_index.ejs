<!DOCTYPE html>
<html lang="en">
<header>
    <% include partials/_header.ejs %>
</header>
<body>
    <% for (id in urlDatabase) {%>
      <li>
        <div class="id">
          <label> <%= id %>: </label>
          <a href="/urls/<%= id %>"> <%= urlDatabase[id] %> </a>
        <form action="/urls/<%= id %>" method="GET">
          <input type="submit" value="Edit">
        </form>
        <form action="/urls/<%= id %>/delete" method="POST">
          <input type="submit" value="Delete">
        </form>
        </div>
      </li>
    <% } %>
</body>
</html>